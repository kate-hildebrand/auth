<h1> Login </h1>

<%# this is how you submit to create action of session %>
<%= form_with :url => "/sessions" do |form| %>
<%#put the form fields inside%>
    <p> 
    <%= form.label "email" %>
    <%= form.text_field "email"%>
    <%= form.label "password"%>
    <%= form.password_field "password" %>
    </p>
    <p>
    <%= form.submit "Login" %>
    </p>

<% end %>